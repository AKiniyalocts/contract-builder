<h1>Contract List</h1>
 
<table>
  <tr>
    <td><b>Company Name</b></td>
    <td><b>Account Number</b></td>
  </tr>
  <% @contract.each do |contract| %>
    <tr>
      <td><%= contract.compName %></td>
      <td><%= contract.accountNum %></td>
    </tr>
  <% end %>
</table>
