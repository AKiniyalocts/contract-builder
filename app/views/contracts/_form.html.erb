
<%= form_for @contract do |f| %>
      <% if @contract.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@contract.errors.count, "error") %> prohibited
      this post from being saved:</h2>
    <ul>
    <% @contract.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
    <% end %>
  </div>

    <h2>About You</h2>
    
    <table>
    <tr>
        <td>Company Name: </td>
        <td><%= f.text_field :compName %></td>
        <td><div class="table_head">Owner or Admin Contact: </div></td>
        <td><%= text_field_tag(:adminContact)%></td>
        <td><div class = "table_head">Computer/Hardware Contact: </td>
        <td><%= text_field_tag(:hard_contact)%></td>
    </tr>
    <tr>
        <td>Account Number: </td>
        <td><%= f.text_field :accountNum %></td>
        <td>Contact Phone: </td>
        <td><%= text_field_tag(:admin_phone)%></td>
        <td>Contact Phone: </td>
        <td><%= text_field_tag(:hard_phone)%></td>
    </tr>
    <tr>
        <td>Company Phone: </td>
        <td><%= f.text_field :compPhone %></td>
        <td>Contact Email: </td>
        <td><%= text_field_tag(:admin_email)%></td>
        <td>Contact Email: </td>
        <td><%= text_field_tag(:hard_email)%></td>
    </tr>
    <tr>
        <td>Company Fax: </td>
        <td><%= f.text_field :compFax %></td>
    </tr>
    </table>
    
   <h2>Shipping Address</h2>
  <table>
    <tr>
        <td>Street: </td>
        <td><%= f.text_field :shipStr %></td>
        <td>County: </td>
        <td><%= f.text_field(:shipCounty)%></td>
